<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomSeekBar">
        <!-- 背景进度颜色 -->
        <attr name="csb_bg" format="color" />
        <!-- 已有进度颜色 -->
        <attr name="csb_progress_bg" format="color" />
        <!-- 进度条的高度 -->
        <attr name="csb_progress_height" format="integer|float|dimension" />
        <!-- 滑块的颜色 -->
        <attr name="csb_thumb_color" format="color" />
        <!-- 滑块的半径 -->
        <attr name="csb_thumb_radius" format="integer|float|dimension" />
        <!-- 进度的文字颜色 -->
        <attr name="csb_progress_text_color" format="color" />
        <!-- 进度的文字大小 -->
        <attr name="csb_progress_text_size" format="integer|float|dimension|reference" />
        <!-- 当前进度值 -->
        <attr name="csb_progress" format="integer" />
        <!-- 最小进度值 -->
        <attr name="csb_min_progress" format="integer" />
        <!-- 最大进度值 -->
        <attr name="csb_max_progress" format="integer" />
        <!-- 是否有进度文字 -->
        <attr name="csb_has_progress_text" format="boolean" />
    </declare-styleable>
</resources>